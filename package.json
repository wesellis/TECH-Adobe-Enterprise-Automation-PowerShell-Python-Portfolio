{
  "name": "adobe-enterprise-automation",
  "version": "2.0.0",
  "description": "Enterprise-scale Adobe Creative Cloud automation system",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "monitor": "node monitoring/server.js"
  },
  "keywords": [
    "adobe",
    "creative-cloud",
    "automation",
    "enterprise",
    "user-management",
    "license-optimization"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "node-cron": "^3.0.2",
    "redis": "^4.6.5",
    "mssql": "^9.1.1",
    "prom-client": "^14.2.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.1"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "nodemon": "^2.0.22",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0",
    "@types/node": "^20.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}